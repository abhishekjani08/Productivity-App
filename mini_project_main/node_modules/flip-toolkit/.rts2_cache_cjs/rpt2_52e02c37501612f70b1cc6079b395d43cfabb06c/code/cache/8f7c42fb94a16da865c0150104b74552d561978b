{"code":"import { isObject, assign } from '../utilities';\r\n// adapted from\r\n// https://github.com/chenglou/react-motion/blob/master/src/presets.js\r\nexport const springPresets = {\r\n    noWobble: { stiffness: 200, damping: 26 },\r\n    gentle: { stiffness: 120, damping: 14 },\r\n    veryGentle: { stiffness: 130, damping: 17 },\r\n    wobbly: { stiffness: 180, damping: 12 },\r\n    stiff: { stiffness: 260, damping: 26 }\r\n};\r\nfunction argIsSpringConfig(arg) {\r\n    return isObject(arg);\r\n}\r\nexport const normalizeSpring = (spring) => {\r\n    if (argIsSpringConfig(spring)) {\r\n        return spring;\r\n    }\r\n    else if (Object.keys(springPresets).indexOf(spring) > -1) {\r\n        return springPresets[spring];\r\n    }\r\n    else {\r\n        return {};\r\n    }\r\n};\r\nexport const getSpringConfig = ({ flipperSpring, flippedSpring } = {}) => {\r\n    return assign({}, springPresets.noWobble, normalizeSpring(flipperSpring), normalizeSpring(flippedSpring));\r\n};\r\n//# sourceMappingURL=index.js.map","map":"{\"version\":3,\"file\":\"index.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../src/springSettings/index.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,QAAQ,EAAE,MAAM,EAAE,MAAM,cAAc,CAAA;AAG/C,eAAe;AACf,sEAAsE;AACtE,MAAM,CAAC,MAAM,aAAa,GAAkB;IAC1C,QAAQ,EAAE,EAAE,SAAS,EAAE,GAAG,EAAE,OAAO,EAAE,EAAE,EAAE;IACzC,MAAM,EAAE,EAAE,SAAS,EAAE,GAAG,EAAE,OAAO,EAAE,EAAE,EAAE;IACvC,UAAU,EAAE,EAAE,SAAS,EAAE,GAAG,EAAE,OAAO,EAAE,EAAE,EAAE;IAC3C,MAAM,EAAE,EAAE,SAAS,EAAE,GAAG,EAAE,OAAO,EAAE,EAAE,EAAE;IACvC,KAAK,EAAE,EAAE,SAAS,EAAE,GAAG,EAAE,OAAO,EAAE,EAAE,EAAE;CACvC,CAAA;AAED,SAAS,iBAAiB,CACxB,GAAmD;IAEnD,OAAO,QAAQ,CAAC,GAAG,CAAC,CAAA;AACtB,CAAC;AAED,MAAM,CAAC,MAAM,eAAe,GAAG,CAC7B,MAAiD,EACjD,EAAE;IACF,IAAI,iBAAiB,CAAC,MAAM,CAAC,EAAE;QAC7B,OAAO,MAAM,CAAA;KACd;SAAM,IAAI,MAAM,CAAC,IAAI,CAAC,aAAa,CAAC,CAAC,OAAO,CAAC,MAAM,CAAC,GAAG,CAAC,CAAC,EAAE;QAC1D,OAAO,aAAa,CAAC,MAAM,CAAC,CAAA;KAC7B;SAAM;QACL,OAAO,EAAE,CAAA;KACV;AACH,CAAC,CAAA;AAED,MAAM,CAAC,MAAM,eAAe,GAAG,CAAC